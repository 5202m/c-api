var logger = require('../resources/logConf');
var app =require('express')();
var socketService = require('../service/socketService');
//配置框架日志输出
logger.initConfig("pm_socket");
var chatSocketServer=app.listen('3007', function() {
    console.log('Chat socket multi server running. Listening on port 3007');
});
socketService.setSocket(chatSocketServer);


