#!/usr/bin/env node

/**
 * Module dependencies.
 */
var app = require('../app');
var debug = require('debug')('pm_api:server');
var taskService = require('../service/taskService');

//启动服务
var apiServer=app.listen('3000',function(){
    console.log('API server running. Listening on port ' + apiServer.address().port);
    //开启定时器
    taskService.start();
});
